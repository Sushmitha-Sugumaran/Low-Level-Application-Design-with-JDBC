Table: user
Columns:
id int AI PK
uname varchar(200)
phonenum varchar(200)
wallet int
email varchar(50)
pass varchar(50)

Table: movies
Columns:
id int AI PK
mschedule varchar(200)
mTime varchar(200)
mlan varchar(200)
mname varchar(200)
mgenre varchar(200)
seatCount int
perTicketprice int

Table: events
Columns:
id int AI PK
eschedule varchar(200)
eTime varchar(200)
elan varchar(200)
ename varchar(200)
egenre varchar(200)
seatCount int
perTicketprice int

Table: bookedhistory
Columns:
u_id int
booking_id int AI PK
showTime varchar(200)
todayTime varchar(200)
u_name varchar(200)
m_name varchar(200)
m_lang varchar(200)
tickets int
amount int